{
  "name": "vitron",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "npx tsc --noEmit",
    "build": "npx tsc --build",
    "dev": "npx tsc --watch"
  },
  "keywords": [
    "build",
    "dev"
  ],
  "bin": {
    "vitron": "bin/vitron"
  },
  "exports": {
    ".": "./dist/index.js",
    "./electron": "./dist/electron/index.js",
    "./store": "./dist/store/index.js",
    "./client": "./dist/client/index.js",
    "./renderer": "./dist/renderer/index.js"
  }
}